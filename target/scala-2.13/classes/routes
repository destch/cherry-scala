# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET	/getdeal		    controllers.HomeController.getDeal(id: String)
GET     /getdeals		    controllers.HomeController.getDeals
GET     /getcurrentdeals		    controllers.HomeController.getCurrentDeals
POST    /deletedeal		    controllers.HomeController.deleteDeal
POST    /updatedeal		    controllers.HomeController.updateDeal
POST    /createdeal		    controllers.HomeController.createDeal

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(file)
